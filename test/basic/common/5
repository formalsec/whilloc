function factorial(x) {
    y = 1;
    res = 1;
    while (y<=x) {
        res = res*y;
        y = y+1
    };
    if (false) {
        print(false)
    }
    else {
        return res
    };
    return -1;
    print(true)
};

function aenima() {
    a = factorial(4);
    print(a);
    return 12;
    print(a);
    print(a)
}